#:kivy 1.8.0

<MenuScreen>:
    BoxLayout:
        Button:
            text: 'Goto Camera'
            on_press: app.load_screen('camera')
        Button:
            text: 'Goto Scanner'
            on_press: app.load_screen('scanner2')
        Button:
            text: 'Quit'
            on_press: app.stop()

